#include <iostream>
#include <iomanip>
using namespace std;

int main() {

	int counts[10];
	int num;

	for (int i = 0; i < 10; i++) {
		counts[i] = 0;
	}

	for (int i = 0; i < 100; i++) {
		num = rand() % 10;
		counts[num]++;
	}

	for (int j = 0; j < 10; j++) {
		cout << counts[j] << " ";
	}


	return 0;
}
